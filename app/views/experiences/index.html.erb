<ul>
    <% @experiences.each do |experience| %>
        <li>
            <strong><%= experience.title %> </strong>
            in <%= experience.location %>: 
            <%= price(experience) %>
        </li>
        <p><%= experience.description %></p>
        <p><%= day_and_time(experience) %></p>
    <% end %>
</ul>